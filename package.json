{
  "name": "@gataca/qr",
  "version": "3.0.10",
  "description": "Gataca component to display presentation requests in QR",
  "author": "Gataca <it@gataca.io>",
  "licenses": [
    {
      "license": "MIT"
    }
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/gatacaqr/gatacaqr.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "audit": "./audit.sh",
    "build": "stencil build --docs",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,jsx}'",
    "generate": "stencil generate",
    "licenses": "npx license-checker --direct --relativeLicensePath > licenses.txt && npx license-checker --direct --summary > licenses-summary.txt",
    "start": "stencil build --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "typescript": "tsc --noEmit --composite false"
  },
  "devDependencies": {
    "@stencil/core": "^2.9.0",
    "@stencil/sass": "^1.4.0",
    "@types/jest": "^27.0.3",
    "@types/react": "^18.2.67",
    "jest": "^27.0.3",
    "jest-cli": "^27.4.5",
    "prettier": "^3.5.3",
    "puppeteer": "^10.0.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "typescript": "^5.8.3"
  },
  "license": "MIT",
  "dependencies": {
    "@stencil/react-output-target": "^0.5.3",
    "pre-commit": "^1.2.2",
    "qr-code-styling": "1.6.0-rc.1"
  },
  "pre-commit": [
    "format",
    "typescript",
    "audit",
    "licenses"
  ]
}